{
	"Info": {
		"creator": "Jannis.Maron@uni-siegen.de",
		"created": "30-September-2025",
		"description": "Sample Config for Raw Coded WFS Reconstruction Optimization",
		
		"30-September-2025": "Created"
	},
	
	"ReconOpt":{
		
		"gt_transforms": {
			"field": {},
			"amp": {
				"sqrt":{}
			},
			"phase": {
				"subtract_mean": {}
				}
		},
		
		"sim_transforms": {
			"field": {
				"crop": {"x_min":125, "y_min":125, "x_max":-125, "y_max":-125}
			},
			"amp": {},
			"phase": {
				"subtract_mean": {}
			}
		},
	
		
		
		"epochs": 3,
		
		"optimizer": {
			"Voxel Object": {"lr": 1e-3}
		},
		"scheduler": {
			"milestones": [40,45],
			"gamma": 0.5
		},
		
		
		"loss_fn": "mse_ncc_loss",
		"weights": [100, 50, 0.5, 150],
		
		"regularizers" : {
			"TV Reg": {"lambda": 5e-5}
		}
		
	}
	
	
	
}